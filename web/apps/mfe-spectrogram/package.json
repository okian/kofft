{
  "name": "mfe-spectrogram",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5175",
    "test": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:rapid": "playwright test --config=playwright.rapid.config.ts",
    "test:e2e:smoke": "playwright test --config=playwright.rapid.config.ts --grep='smoke'",
    "test:e2e:quick": "playwright test --config=playwright.rapid.config.ts quick-check.spec.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "test:e2e:install-deps": "playwright install-deps",
    "test:e2e:setup": "node scripts/setup-e2e.js",
    "clean:test-artifacts": "./scripts/clean-test-artifacts.sh",
    "test:e2e:parallel": "playwright test --workers=4",
    "test:e2e:parallel-max": "playwright test --workers=8",
    "test:e2e:parallel-ci": "playwright test --workers=2",
    "test:e2e:shard": "playwright test --shard=1/3",
    "test:e2e:shard-2": "playwright test --shard=2/3",
    "test:e2e:shard-3": "playwright test --shard=3/3",
    "clean": "npm run clean:build && npm run clean:test && npm run clean:cache && npm run clean:logs",
    "clean:build": "rm -rf dist/ build/ .vite/",
    "clean:test": "rm -rf test-results/ allure-results/ playwright-report/ coverage/ .nyc_output/",
    "clean:cache": "rm -rf node_modules/.cache/ .cache/ .parcel-cache/ .eslintcache/ .stylelintcache/",
    "clean:logs": "find . -name '*.log' -delete && find . -name '*.tmp' -delete && find . -name '*.temp' -delete",
    "clean:all": "npm run clean && rm -rf node_modules/ package-lock.json && npm run clean:os",
    "clean:os": "find . -name '.DS_Store' -delete && find . -name 'Thumbs.db' -delete && find . -name '*.swp' -delete && find . -name '*.swo' -delete",
    "clean:wasm": "rm -rf wasm/target/ wasm/pkg/ src/wasm/target/ src/wasm/pkg/",
    "clean:deep": "npm run clean:all && npm run clean:wasm && find . -name '*.orig' -delete && find . -name '*.rej' -delete && find . -name '*.bak' -delete"
  },
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "allure-playwright": "^3.3.3",
    "clsx": "^2.1.1",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.446.0",
    "music-metadata": "^11.8.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-window": "^1.8.11",
    "tailwind-merge": "^2.5.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.3.7",
    "@playwright/test": "^1.42.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "canvas": "^3.0.0",
    "fast-check": "^3.14.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "^3.2.4"
  }
}
